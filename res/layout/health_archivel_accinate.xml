                android:layout_height="wrap_content"
                android:layout_margin="5dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="迁入时间："
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <com.health.util.DateEditText
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text="1977年05月08"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="迁出时间："
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <com.health.util.DateEditText
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text="1977年05月08"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="迁出原因"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <EditText
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text="无"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="疫苗异常反应史："
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="接种禁忌："
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="传染病史："
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="建卡日期："
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <com.health.util.DateEditText
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text="1977年05月08"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="50dp"
                    android:text="建卡人:"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <EditText
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />
            </LinearLayout>

            <com.health.util.ListViewForScrollView
                android:id="@+id/vaccinate_list_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" 
                android:layout_marginRight="50dp"/>
        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/edit_help_button"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginRight="50dp"
        android:background="@android:color/transparent" />

</RelativeLayout>