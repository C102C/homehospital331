<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fadeScrollbars="false"
        android:orientation="vertical"
        android:background="@color/shallow_blue"
        android:id="@+id/table_body_sv"
        android:paddingRight="50dp" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp" >

                <TextView
                    android:id="@+id/name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="姓名:"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <EditText
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/name"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text="小二"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/serial_id"
                    android:text="编号:"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <TextView
                    android:id="@+id/serial_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:text="2001987-001"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp" >

                <TextView
                    android:id="@+id/sex"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="性别:"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <EditText
                    android:id="@+id/sex_et"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text="男"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="出生日期："
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <com.health.util.DateEditText
                    android:layout_width="220dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text="1977年05月08"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp" >

                <TextView
                    android:id="@+id/sex"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="监护人姓名:"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

                <EditText
                    android:id="@+id/sex_et"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/archive_edit_bg"
                    android:gravity="center"
                    android:text="大二"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_3" />

